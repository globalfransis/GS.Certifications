{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "datatables.net@1.10.25",
      "destination": "wwwroot/lib/datatables.net/"
    },
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/"
    },
    { 
      "library": "datatables.net-bs4@1.10.25",
      "destination": "wwwroot/lib/datatables.net-bs4/"
    },
    {
      "library": "twitter-bootstrap@5.0.1",
      "destination": "wwwroot/lib/twitter-bootstrap/"
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "library": "font-awesome@5.15.1",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "popper.js@2.6.0",
      "destination": "wwwroot/lib/popper/"
    },
    {
      "library": "open-iconic@1.1.1",
      "destination": "wwwroot/lib/open-iconic/"
    },
    {
      "provider": "jsdelivr",
      "library": "@riophae/vue-treeselect@0.4.0",
      "destination": "wwwroot/lib/vue-treeselect"
    },
    {
      "provider": "jsdelivr",
      "library": "vue@2.6.12",
      "destination": "wwwroot/lib/vue"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "vuedraggable@2.24.3",
      "destination": "wwwroot/lib/vue-draggable"
    }
  ]
}